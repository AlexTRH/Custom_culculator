{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parser": "babel-eslint",
    "parserOptions": {
            "ecmaFeatures": {
                "jsx": true
            },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [ 
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
