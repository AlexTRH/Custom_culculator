{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "prettier"],
	"overrides": [],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"requireConfigFile": false,
		"babelOptions": {
			"babelrc": false,
			"configFile": false,
			"presets": ["@babel/preset-env"]
		},
		"ecmaFeatures": {},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"parser": "flow",
				"endOfLine": "auto"
			}
		],
		"linebreak-style": "off",
		"indent": "off",
		"spaced-comment": "off",
		"no-restricted-globals": "off",
		"no-param-reassign": "off",
		"no-restricted-syntax": "off",
		"import/prefer-default-export": 0,
		"class-methods-use-this": 0,
		"max-classes-per-file": 0,
		"no-undef": 0
	}
}
